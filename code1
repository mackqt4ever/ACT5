class Person:
    def __init__(self, name, age):
        self.__name = name  
        self.__age = age    
   
    def get_name(self):
        return self.__name
   
    def set_name(self, name):
        self.__name = name

p = Person("Mark", 20)

print(p.get_name())  

p.set_name("Heidi")
print(p.get_name())  
